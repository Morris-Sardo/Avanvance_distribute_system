{
  "system": {
    "name": "Advanced Distributed System",
    "version": "1.0.0",
    "environment": "development"
  },
  "services": {
    "registry": {
      "host": "localhost",
      "port": 8761,
      "healthCheckInterval": 30000
    },
    "loadBalancer": {
      "host": "localhost",
      "port": 8080,
      "algorithm": "round-robin"
    },
    "apiGateway": {
      "host": "localhost",
      "port": 8000,
      "timeout": 30000
    }
  },
  "database": {
    "type": "mongodb",
    "host": "localhost",
    "port": 27017,
    "name": "distributed_system",
    "poolSize": 10
  },
  "cache": {
    "type": "redis",
    "host": "localhost",
    "port": 6379,
    "ttl": 3600
  },
  "messageQueue": {
    "type": "rabbitmq",
    "host": "localhost",
    "port": 5672,
    "exchange": "distributed_system"
  },
  "logging": {
    "level": "info",
    "format": "json",
    "outputs": ["console", "file"]
  },
  "monitoring": {
    "enabled": true,
    "metricsPort": 9090,
    "healthCheckPath": "/health"
  },
  "security": {
    "enableAuth": true,
    "tokenExpiry": 3600,
    "corsEnabled": true
  }
}
